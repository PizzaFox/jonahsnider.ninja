	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "1.2.32",
		"@fortawesome/free-brands-svg-icons": "5.15.1",
		"@fortawesome/react-fontawesome": "0.1.13",
		"@material-ui/core": "4.11.0",
		"@material-ui/icons": "4.9.1",
		"firebase": "8.1.1",
		"next": "10.0.3",
		"next-compose-plugins": "2.2.1",
		"next-pwa": "3.1.5",
		"react": "17.0.1",
		"react-dom": "17.0.1",
		"typescript": "4.0.3"
	},
	"devDependencies": {
		"@types/node": "12.19.4",
		"@types/react": "17.0.0",
		"eslint-config-xo-react": "0.23.0",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "7.21.5",
		"eslint-plugin-react-hooks": "4.2.0",
		"netlify-plugin-cache-nextjs": "1.5.2",
		"prettier": "2.1.2",
		"xo": "0.34.2"
	},
	"name": "jonah.pw",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"export": "next export",
		"lint": "xo",
		"preexport": "rm -rf out",
		"start": "next start",
		"style": "prettier --ignore-path .gitignore --check ."
	},
	"version": "3.0.0",
	"xo": {
		"extends": [
			"xo",
			"xo/esnext",
			"xo/browser",
			"xo-react"
		],
		"ignores": [
			"public"
		],
		"overrides": [
			{
				"extends": [
					"xo"
				],
				"files": "*.config.js"
			}
		],
		"prettier": true,
		"rules": {
			"import/no-unassigned-import": [
				"error",
				{
					"allow": [
						"firebase/*",
						"firebase"
					]
				}
			],
			"react/no-unescaped-entities": "off",
			"react/prop-types": "off"
		}
	}
}
