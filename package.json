	"dependencies": {
		"firebase": "8.2.10",
		"next": "10.0.8",
		"next-compose-plugins": "2.2.1",
		"next-pwa": "5.0.6",
		"next-seo": "4.20.0",
		"next-themes": "0.0.12",
		"normalize.css": "8.0.1",
		"react": "17.0.1",
		"react-dom": "17.0.1",
		"sass": "1.32.8",
		"typescript": "4.1.3"
	},
	"devDependencies": {
		"@types/node": "12.20.4",
		"@types/react": "17.0.2",
		"eslint-config-xo-react": "0.24.0",
		"eslint-plugin-prettier": "3.3.1",
		"eslint-plugin-react": "7.22.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"netlify-plugin-cache-nextjs": "1.5.2",
		"netlify-plugin-cache-yarn2": "1.0.0",
		"prettier": "2.2.1",
		"webpack": "webpack-4",
		"xo": "0.37.1"
	},
	"name": "jonah.pw",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"export": "next export",
		"lint": "xo",
		"preexport": "rm -rf out",
		"start": "next start",
		"style": "prettier --check ."
	},
	"version": "3.0.0",
	"xo": {
		"extends": [
			"xo",
			"xo/esnext",
			"xo/browser",
			"xo-react"
		],
		"ignores": [
			"public"
		],
		"overrides": [
			{
				"extends": [
					"xo"
				],
				"files": "*.config.js"
			}
		],
		"prettier": true,
		"rules": {
			"import/no-unassigned-import": [
				"error",
				{
					"allow": [
						"firebase/*",
						"firebase"
					]
				}
			],
			"react/no-unescaped-entities": "off",
			"react/prop-types": "off"
		}
	}
}
